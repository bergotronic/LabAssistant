<?xml version="1.0" encoding="UTF-8"?>
<Experiments>
  <Experiment>
    <Name>Hello World Local Version</Name>
    <Description>Local Version of Hello World - Writes a Text output to your local hybrid runbook worker</Description>
    <RunbookName>Hello_World_Write-File</RunbookName>
    <Input1>Text to Write</Input1>
    <Input2>File to Write</Input2>
  </Experiment>

  <Experiment>
    <Name>LAB_DeployVagrantBox</Name>
    <Description>Uses Vagrant to deploy a Vagrant Box machine</Description>
    <RunbookName>LAB_LinuxDeploy1</RunbookName>
    <Input1>Box Name</Input1>
    <Input2>Host Folder Name</Input2>
  </Experiment>

  <Experiment>
    <Name>LAB_LabStatus</Name>
    <Description>Gets Status of all Hyper-V Hosts, and sends an email with status</Description>
    <RunbookName>LAB_LabStatus</RunbookName>
    <Input1>Recipient</Input1>
  </Experiment>


  <Experiment>
    <Name>Docker Deployment</Name>
    <Description>Uses SSH to Deploy a Docker Container to a Docker Linux Host</Description>
    <RunbookName>LAB_DockerDeploy</RunbookName>
    <Input1>MachineName</Input1>
    <Input2>Docker Container</Input2>
  </Experiment>
  
  
  <Experiment>
    <Name>LAB_DeployVagrantLab</Name>
    <Description>Deploys a whole lab!</Description>
    <RunbookName>LAB_DeployVagrantLab</RunbookName>
  </Experiment>
  
  <Experiment>
    <Name>LAB_DestroyVagrantLab</Name>
    <Description>Destroys the lab!</Description>
    <RunbookName>LAB_DestroyVagrantLab</RunbookName>
  </Experiment>


  <HybridWorkerGroup>
    <Name>Azure</Name>
  </HybridWorkerGroup>

  <HybridWorkerGroup>
    <Name>BC-HybridWorker1</Name>
  </HybridWorkerGroup>

  <HybridWorkerGroup>
    <Name>LEE-NUC1</Name>
  </HybridWorkerGroup>

  <WebHook>
    <Name></Name>
  </WebHook>

</Experiments>